<%- include('partials/header') %>


<a style=" font-family: 'Open Sans', sans-serif; font-size: 0.9rem;" ><%=loginUserName%></a>
<a style=" font-family: 'Open Sans', sans-serif; font-size: 0.7rem;" href="/logout" role="button">Log Out</a>
</div>
<div class="container mt-5">
  <h1>Google Register</h1>

  <div class="row">
    <div class="col-sm-8">
      <div class="card">
        <div class="card-body">

          <!-- Makes POST request to /register route -->
          <form action="/register" method="POST">
            <div class="form-group">
              <label for="institutename">Institute Name</label>
              <input type="text" class="form-control" name="userinstitutename">
            </div>
            <div class="form-group">
              <label for="id">Institute ID</label>
              <input type="number" class="form-control" name="userid">
            </div>
            <div class="form-group">
              <label for="whatsappno">Whatsapp Number</label>
              <input type="number" class="form-control" name="usernumber">
            </div>
            
            <button type="google_registration_submit" class="btn btn-dark">Complete Register</button>
          </form>

        </div>
      </div>
    </div>

    

  </div>
</div>

<%- include('partials/footer') %>
